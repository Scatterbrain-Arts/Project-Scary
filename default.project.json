{
  "name": "Project-Scary",
  "globIgnorePaths": [ "**/.package-lock.json" ],
  "tree": {
    "$className": "DataModel",

    "ServerScriptService": {
      "$className": "ServerScriptService",
      "src": {
        "$className": "Folder",
        
        "Game": {
          "$path": "src/game"
        },
        "packages": {
          "$path": "node_modules/@quenty"
        }
      },

      "Main": {
        "$path": "src/scripts/server/main"
      }
    },
    
    "StarterPlayer": {
      "StarterPlayerScripts": {
        "Main": {
          "$path": "src/scripts/client/player"
        }
      },

      "StarterCharacterScripts": {
        "Main": {
          "$path": "src/scripts/client/character"
        }
      }
    },

    "ReplicatedStorage": {
      "Tasks": {
        "$path": "src/tasks"
      }
    }
  }
}